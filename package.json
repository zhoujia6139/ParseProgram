{
  "name": "electric-metric",
  "version": "1.0.0",
  "private": true,
  "description": "private UI for electric metric display",
  "homepage": "https://localhost:4001",
  "main": "index.js",
  "scripts": {
    "tsc": "./node_modules/typescript/bin/tsc",
    "tsc:watch": "./node_modules/typescript/bin/tsc -w",
    "new:component": "node ./scripts/generateComponent.js",
    "dev": "concurrently -kill-others \"npm run start\" \"npm run start:dev --prefix backend\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "ENABLE_FULL_ROUTES=true webpack-dev-server --config webpack.config.js --mode development --hot --progress --color --port 4000 --open",
    "build": "webpack -p --progress --colors",
    "storybook": "IS_STORYBOOK=true start-storybook -p 6001",
    "prettier": "prettier --write \"src/**/*.{ts,tsx}\"",
    "build:docker": "docker build -t texas .",
    "deploy:docker": "docker run -d --name texas1 --restart unless-stopped -p 4000:4000 -p 4001:4001 texas",
    "build-storybook": "build-storybook"
  },
  "proxy": "http://localhost:4001",
  "license": "MIT",
